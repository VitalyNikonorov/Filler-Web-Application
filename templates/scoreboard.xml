<fest:template xmlns:fest="http://fest.mail.ru"
    context_name="json">
    <fest:include src='addons/header.xml'/>
    <div>
        <h3>Score Board</h3>
    </div>

    <table class="table">
        <tr><th>name</th> <th>score</th></tr>
            <fest:for iterate="json" index="i">
            <tr>
                <td><fest:value>json[i]["Name"]</fest:value></td>
                <td><fest:value>json[i]["score"]</fest:value></td>
            </tr>
            </fest:for>

    </table>
    <a href="#" class="btn btn-danger">back</a>
</fest:template>